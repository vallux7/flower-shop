package flowerproject;

public class ArtificialFlowers extends Flowers {

    public ArtificialFlowers() {

    }

    public ArtificialFlowers(String color, int priceOfPiece, boolean isAvailable, long RequestNumber, String PaymentType, Date RequestDate, int quantity, Date delDate, Customer info, Departments item) {
        super(color, priceOfPiece, isAvailable, RequestNumber, PaymentType, RequestDate, quantity, delDate, info, item);
    }

   

    @Override

    public String getServiceType() {

        return "Artificial Flowers";

    }
}
